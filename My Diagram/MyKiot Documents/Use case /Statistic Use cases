Thống kê hàng hóa 
	1.Thống kê 10 sản phẩm có doanh số cao nhất (tính bằng current_goods_so_luong)
		bảng : hóa_đơn_và_hàng-hóa 
		join : hang_hoa 
		columns : 
		where gab_thoi_gian 
		groupBy gab_goods_id 
		limit = 10
		orderBy gab_tong_tien_ban
	2.Thống kê 10 sản phẩm có lợi nhuận cao nhất (trừ luôn trả hàng)
		bảng : hóa_đơn_và_hàng_hóa 
		join: hang_hoa 
		columns : 
		where gab_thoi_gian 
		groupBy gab_goods_id 
		limit = 10
		orderBy gab_tong_tien_loi
	Báo cáo nhân viên theo hàng bán 
		bảng : hoa_don
		join : hoa_don_va_hang_hoa,nguoi_lam 
		columns :
		where gab_thoi_gian
		groupBy gab_goods_id
		limit = null
		orderBy sum(personnel_id)
	Báo cáo khách theo hàng bán 
		bảng : hoa_don
		join : hoa_don_va_hang_hoa,khach_hang
		columns : 
		where gab_thoi_gian
		groupBy gab_goods_id
		limit = null
		orderBy sum(customer_id)
	Báo cáo nhà cung cấp theo hàng mua 
		bảng: phieu_nhap_hang 
		join : hang_hoa_va_nhap_hang, nha_cung_cap
		columns: 
		where gaic_thoi_gian
		groupBy gaic_goods_id
		limit = null
		orderBy sum(provider_id)
Thống kê khách hàng 
	Top 10 khách hàng mua nhiều nhất (trừ mấy cái trả lại )
		Bảng: hoa_don
		Join: hang_hoa_va_hoa_don , khach_hang
		columns: 
		where gab_thoi_gian
		groupBy  khach_hang.customer_id
		limit 10 
		orderBy sum(hang_hoa_va_hoa_don.gab_tong_tien_ban)
	Báo cáo hàng bán theo khách  
		Bảng: hoa_don
		Join: hang_hoa_va_hoa_don , khach_hang
		columns: 
		where gab_thoi_gian
		groupBy  khach_hang.customer_id
		orderBy sum(hang_hoa_va_hoa_don.gab_current_so_luong)
	Top 10 khách hàng nợ nhiều nhất 
		ddax xong
	Báo cáo lợi nhuận theo khách hàng 
		Bảng: hoa_don
		Join: hang_hoa_va_hoa_don , khach_hang
		columns: 
		where gab_thoi_gian
		groupBy  khach_hang.customer_id
		limit 10 
		orderBy sum(hang_hoa_va_hoa_don.gab_tong_tien_loi)
Thống kê nhà cung cấp 
	1.Top 10 nhà cung cấp nhập hàng nhiều nhất 
		Bảng : hang_hoa_va_nhap_hang
		Join :  phieu_nhap_hang , nha_cung_cap
		Columns:  nha_cung_cap.provider_ten , sum (hang_hoa_va_nhap_hang.gaic_tong_tien_ban) tong_tien_ban
		where :  gab_thoi_gian
		groupBy: nha_cung_cap.provider_id 
		limit : 10
		orderBy: sum (hang_hoa_va_nhap_hang.gaic_tong_tien_ban)
	2.Báo cáo số hàng nhập theo từng nhà cung cấp 
		Bảng : hang_hoa_va_nhap_hang
		Join :  nha_cung_cap, phieu_nhap_hang
		Columns:  nha_cung_cap.provider_ten,sum(hang_hoa_va_nhap_hang.gaic_current_so_luong) tong_so_luong,sum(hang_hoa_va_nhap_hang.gaic_tong_tien_ban) tong_tien_ban
		where :  
		groupBy: 
		limit : 
		orderBy: 
	3.top 10 nhà cung cấp bên mình nợ nhiều nhất 
		đã hoàn tất 	


Thống kê nhân viên 
	Báo cáo thong tin bán hàng của mỗi nhân viên (nhân viên + doanh thu)
		Bảng : hang_hoa_va_hoa_don 
		Join : hoa_don, nguoi_lam 
		columns: nguoi_lam.personnel_ten ,sum(hang_hoa_va_hoa_don.gab_tong_so_luong) tong_so_luong , sum(hang_hoa_va_hoa_don.gab_tong_tien_ban) tong_tien_ban , sum(hang_hoa_va_hoa_don.gab_tong_tien_von) tong_tien_von , sum(hang_hoa_va_hoa_don.gab_tong_tien_loi) tong_tien_loi
		groupBy : nguoi_lam.personnel_id
		limit : null
		orderBy :  sum(hang_hoa_va_hoa_don.gab_tong_tien_ban)
	Báo cáo danh sách hàng bán của mỗi nhân viên (nhân viên + số lượng bán + tổng số tiền )
		Bảng : hang_hoa_va_hoa_don 
		Join : hoa_don, nguoi_lam 
		columns: nguoi_lam.personnel_ten ,sum(hang_hoa_va_hoa_don.gab_tong_so_luong) tong_so_luong , sum(hang_hoa_va_hoa_don.gab_tong_tien_ban) tong_tien_ban , sum(hang_hoa_va_hoa_don.gab_tong_tien_von) tong_tien_von , sum(hang_hoa_va_hoa_don.gab_tong_tien_loi) tong_tien_loi
		groupBy : nguoi_lam.personnel_id
		limit : null
		orderBy : sum(hang_hoa_va_hoa_don.gab_tong_so_luong) 
	Báo cáo lợi nhuận mà mỗi nhân viên tạo ra (nhân viên + tổng tiền bán + tổng tiền vốn + tổng tiền lời )
		Bảng : hang_hoa_va_hoa_don 
		Join : hoa_don, nguoi_lam 
		columns: nguoi_lam.personnel_ten ,sum(hang_hoa_va_hoa_don.gab_tong_so_luong) tong_so_luong , sum(hang_hoa_va_hoa_don.gab_tong_tien_ban) tong_tien_ban , sum(hang_hoa_va_hoa_don.gab_tong_tien_von) tong_tien_von , sum(hang_hoa_va_hoa_don.gab_tong_tien_loi) tong_tien_loi
		groupBy : nguoi_lam.personnel_id
		limit : null
		orderBy :sum(hang_hoa_va_hoa_don.gab_tong_tien_loi) 
	


